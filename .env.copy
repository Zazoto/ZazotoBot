# Wallet
dddd
# Connection
RPC_ENDPOINT=https://api.mainnet-beta.solana.com
RPC_WEBSOCKET_ENDPOINT=wss://api.mainnet-beta.solana.com
COMMITMENT_LEVEL=confirmed

# Bot
LOG_LEVEL=trace
ONE_TOKEN_AT_A_TIME=true
PRE_LOAD_EXISTING_MARKETS=false
CACHE_NEW_MARKETS=false
TRANSACTION_EXECUTOR=warp           # Use Warp for faster execution
CUSTOM_FEE=0.006                   # Prioritize transactions with higher custom fee

# Buy
QUOTE_MINT=So11111111111111111111111111111111111111112  # WSOL mint address
QUOTE_AMOUNT=0.05                                      # Use 0.05 WSOL per trade
AUTO_BUY_DELAY=0                                       # Instant buy
MAX_BUY_RETRIES=999                                    # Retry indefinitely for buys
BUY_SLIPPAGE=0.3                                       # Lower slippage for better efficiency

# Sell
AUTO_SELL=true                                         # Enable automatic selling
MAX_SELL_RETRIES=999                                   # Retry indefinitely for sells
AUTO_SELL_DELAY=0                                      # Instant sell
PRICE_CHECK_INTERVAL=1                                 # Check price every millisecond
PRICE_CHECK_DURATION=0                                 # Always monitor price until profitable
TAKE_PROFIT=0.3                                        # Minimum profit threshold at 0.3% (ensures profit > fees)
STOP_LOSS=0                                            # Disable stop loss to prevent selling at a loss
SELL_SLIPPAGE=0.3                                      # Lower slippage for better sell price

# Filters
USE_SNIPE_LIST=false                                   # Disable snipe list for open trading
SNIPE_LIST_REFRESH_INTERVAL=30000                     # Default refresh interval
FILTER_CHECK_DURATION=1000                             # Filter checks every second
FILTER_CHECK_INTERVAL=1                                # Real-time filter checks
CONSECUTIVE_FILTER_MATCHES=1                           # Immediate match on first check
CHECK_IF_MUTABLE=false                                 # No restrictions on metadata mutability
CHECK_IF_SOCIALS=false                                 # Disable social checks
CHECK_IF_MINT_IS_RENOUNCED=true                        # Ensure mint is renounced
CHECK_IF_FREEZABLE=false                               # No restrictions on freezability
CHECK_IF_BURNED=true                                   # Ensure pool is burned
MIN_POOL_SIZE=10                                       # Minimum pool size in WSOL
MAX_POOL_SIZE=100                                      # Maximum pool size in WSOL
